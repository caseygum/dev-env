  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>NUnit Test</Title>
			<Shortcut>nut</Shortcut>
			<Description>Generates an NUnit test method</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>methodName</ID>
					<ToolTip>The name of the test method</ToolTip>
					<Default>NewTest</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[Test]
	public void $methodName$()
	{
		$selected$$end$
	}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
  
  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>NUnit Test with Exception</Title>
			<Shortcut>nutex</Shortcut>
			<Description>Generates an NUnit test method that expects an exception</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
        <Literal>
					<ID>exceptionType</ID>
					<ToolTip>The type of exception to expect</ToolTip>
					<Default>ArgumentNullException</Default>
				</Literal>
				<Literal>
					<ID>methodName</ID>
					<ToolTip>The name of the test method</ToolTip>
					<Default>NewTest</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[Test, ExpectedException(typeof($exceptionType$))]
	public void $methodName$()
	{
		$selected$$end$
	}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
  
  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>Create object under test</Title>
			<Shortcut>cout</Shortcut>
			<Description>Creates an object for testing purposes</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
        <Literal>
					<ID>type</ID>
					<ToolTip>The type of object to create</ToolTip>
					<Default>type</Default>
				</Literal>
				<Literal>
					<ID>creation</ID>
					<ToolTip>The name of the test method</ToolTip>
					<Default>null</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[private $type$ CreateObjectUnderTest()
  {
    return $creation$$end$;
  }]]>
			</Code>
		</Snippet>
	</CodeSnippet>
  
  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>Get object under test</Title>
			<Shortcut>gout</Shortcut>
			<Description>Gets the object returned from the CreateObjectUnderTest method</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
        <Literal>
					<ID>type</ID>
					<ToolTip>The type of object to get</ToolTip>
					<Default>var</Default>
				</Literal>
				<Literal>
					<ID>name</ID>
					<ToolTip>The name of the variable </ToolTip>
					<Default>obj</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[$type$ $name$ = CreateObjectUnderTest();]]>
			</Code>
		</Snippet>
	</CodeSnippet>

  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>New Guid string</Title>
			<Shortcut>guids</Shortcut>
			<Description>Creates a new Guid string</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
        <Literal>
					<ID>var</ID>
					<Default>id</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[string $var$ = Guid.NewGuid().ToString();$end$]]></Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>
