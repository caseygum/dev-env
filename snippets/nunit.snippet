<?xml version="1.0" encoding="utf-8" ?>
<CodeSnippets  xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>NUnit Test Fixture</Title>
			<Shortcut>tf</Shortcut>
			<Description>Generates an NUnit test fixture</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>className</ID>
					<ToolTip>The name of the test class</ToolTip>
					<Default>NewTestFixture</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[TestFixture]]]>
			</Code>
		</Snippet>
	</CodeSnippet>
  
  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>NUnit Fixture Set Up</Title>
			<Shortcut>nufsu</Shortcut>
			<Description>Generates an NUnit fixture set up method</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>methodName</ID>
					<ToolTip>The name of the fixture set up method</ToolTip>
					<Default>FixtureSetUp</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[[TestFixtureSetUp]
	public void $methodName$()
	{
		$selected$$end$
	}]]>
			</Code>
		</Snippet>
	</CodeSnippet>
  
  <CodeSnippet Format="1.0.0">
