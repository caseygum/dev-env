  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>Assert.Contains</Title>
			<Shortcut>acon</Shortcut>
			<Description>Asserts that an object is contained within a given collection</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>object</ID>
					<Default>object</Default>
				</Literal>
				<Literal>
					<ID>collection</ID>
					<Default>collection</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[Assert.Contains($object$, $collection$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
</CodeSnippets>