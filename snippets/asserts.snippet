  <CodeSnippet Format="1.0.0">
		<Header>
			<Title>Assert.AreEqual</Title>
			<Shortcut>aeq</Shortcut>
			<Description>Asserts that two expression values are equal</Description>
			<SnippetTypes>
				<SnippetType>Expansion</SnippetType>
				<SnippetType>SurroundsWith</SnippetType>
			</SnippetTypes>
		</Header>
		<Snippet>
			<Declarations>
				<Literal>
					<ID>val1</ID>
					<Default>val1</Default>
				</Literal>
				<Literal>
					<ID>val2</ID>
					<Default>val2</Default>
				</Literal>
			</Declarations>
			<Code Language="csharp"><![CDATA[Assert.AreEqual($val1$, $val2$);$end$]]>
			</Code>
		</Snippet>
	</CodeSnippet>
